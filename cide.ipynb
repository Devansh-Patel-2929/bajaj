    "import json\n",
    "import re\n",
    "import pandas as pd\n",
    "from datetime import datetime, timezone\n",
    "from collections import Counter\n",
    "from scipy.stats import pearsonr\n",
    "\n",
    "# Configuration\n",
    "REFERENCE_DATE = datetime(2025, 4, 24, tzinfo=timezone.utc)\n",
    "\n",
    "def load_data(filename):\n",
    "    with open(filename) as f:\n",
    "        return json.load(f)\n",
    "\n",
    "def is_valid_phone(phone):\n",
    "    if not phone: return False\n",
    "    cleaned = re.sub(r'\\D', '', phone)\n",
    "    \n",
    "    if len(cleaned) == 10:\n",
    "        return 6000000000 <= int(cleaned) <= 9999999999\n",
    "    elif len(cleaned) == 12 and cleaned.startswith('91'):\n",
    "        return 6000000000 <= int(cleaned[2:]) <= 9999999999\n",
    "    elif len(cleaned) == 13 and cleaned.startswith('91'):\n",
    "        return 6000000000 <= int(cleaned[3:]) <= 9999999999\n",
    "    return False\n",
    "\n",
    "def process_data(data):\n",
    "    records = []\n",
    "    medicine_counter = Counter()\n",
    "    valid_phones = 0\n",
    "    total_meds = 0\n",
    "    patients_with_meds = 0\n",
    "    \n",
    "    for entry in data:\n",
    "        # Phone validation\n",
    "        if is_valid_phone(entry.get('phoneNumber')):\n",
    "            valid_phones += 1\n",
    "        \n",
    "        # Gender processing\n",
    "        patient_details = entry.get('patientDetails', {})\n",
    "        gender = patient_details.get('gender', '').strip().upper() or None\n",
    "        \n",
    "        # Age calculation\n",
    "        age = None\n",
    "        if birth_date_str := patient_details.get('birthDate'):\n",
    "            birth_date = datetime.fromisoformat(birth_date_str.replace('Z', '+00:00'))\n",
    "            age_delta = REFERENCE_DATE - birth_date\n",
    "            age = age_delta.days / 365.25\n",
    "            \n",
    "        # Medicines processing\n",
    "        medicines = entry.get('consultationData', {}).get('medicines', [])\n",
    "        med_names = [med['medicineName'] for med in medicines if med.get('medicineName')]\n",
    "        medicine_counter.update(med_names)\n",
    "        \n",
    "        if medicines:\n",
    "            total_meds += len(medicines)\n",
    "            patients_with_meds += 1\n",
    "        \n",
    "        records.append({\n",
    "            'gender': gender,\n",
    "            'age': age,\n",
    "            'medicines_count': len(medicines)\n",
    "        })\n",
    "    \n",
    "    return records, medicine_counter, valid_phones, total_meds, patients_with_meds\n",
    "\n",
    "def main():\n",
    "    data = load_data('DataEngineeringQ2.json')\n",
    "    records, medicine_counter, valid_phones, total_meds, patients_with_meds = process_data(data)\n",
    "    \n",
    "    df = pd.DataFrame(records)\n",
    "    \n",
    "    # Gender imputation and calculation\n",
    "    mode_gender = df['gender'].mode()[0] if not df['gender'].mode().empty else None\n",
    "    df['gender'] = df['gender'].fillna(mode_gender)\n",
    "    female_percent = (df['gender'] == 'F').mean() * 100\n",
    "    \n",
    "    # Age grouping\n",
    "    bins = [0, 12, 19, 59, 200]\n",
    "    labels = ['Child', 'Teen', 'Adult', 'Senior']\n",
    "    df['ageGroup'] = pd.cut(df['age'], bins=bins, labels=labels, right=True)\n",
    "    adult_count = df['ageGroup'].value_counts().get('Adult', 0)\n",
    "    \n",
    "    # Medicine calculations\n",
    "    avg_meds = total_meds / patients_with_meds if patients_with_meds else 0\n",
    "    sorted_meds = medicine_counter.most_common()\n",
    "    third_most_common = sorted_meds[2][0] if len(sorted_meds) >=3 else None\n",
    "    \n",
    "    # Average age calculation\n",
    "    avg_age = df['age'].mean()\n",
    "    \n",
    "    # Print results\n",
    "    print(f\"Valid phone numbers: {valid_phones}\")\n",
    "    print(f\"Female percentage after imputation: {female_percent:.2f}%\")\n",
    "    print(f\"Adult count: {adult_count}\")\n",
    "    print(f\"Average medicines per patient: {avg_meds:.2f}\")\n",
    "    print(f\"Average age as of 2025-04-24: {avg_age:.2f} years\")\n",
    "    print(f\"3rd most prescribed medicine: {third_most_common}\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
  }
