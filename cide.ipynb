    "import json\n",
    "import re\n",
    "import pandas as pd\n",
    "from scipy.stats import pearsonr\n",
    "from datetime import datetime, timezone\n",
    "\n",
    "# --- PHONE NUMBER VALIDATION FUNCTION ---\n",
    "def is_valid_phone_number(phone):\n",
    "    if not phone:  
    "        return False\n",
    "    \n",
    "    # Clean the number\n",
    "    cleaned = phone.replace(' ', '').replace('-', '')\n",
    "    \n",
    "    # Check prefix and extract core number\n",
    "    if cleaned.startswith('+91'):\n",
    "        core = cleaned[3:]\n",
    "    elif cleaned.startswith('91'):\n",
    "        core = cleaned[2:]\n",
    "    else:\n",
    "        core = cleaned\n",
    "    \n",
    "    # Validate length and numeric format\n",
    "    if not re.fullmatch(r'\\d{10}', core):\n",
    "        return False\n",
    "    \n",
    "    # Convert to integer for range check\n",
    "    num = int(core)\n",
    "    return 6000000000 <= num <= 9999999999\n",
    "\n",
    "# --- DATA PROCESSING ---\n",
    "with open('DataEngineeringQ2.json') as f:\n",
    "    data = json.load(f)  # Root is a list of patient records\n",
    "\n",
    "valid_phones = 0\n",
    "records = []\n",
    "\n",
    "for entry in data:\n",
    "    # --- Phone Validation ---\n",
    "    phone = entry.get('phoneNumber')\n",
    "    if is_valid_phone_number(phone):\n",
    "        valid_phones += 1\n",
    "    \n",
    "    # --- Age/Medication Data for Correlation ---\n",
    "    patient_details = entry.get('patientDetails', {})\n",
