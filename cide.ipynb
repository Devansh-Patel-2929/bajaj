    "import json\n",
    "import re\n",
    "import pandas as pd\n",
    "from scipy.stats import pearsonr\n",
    "from datetime import datetime, timezone\n",
    "\n",
    "def is_valid_phone_number(phone):\n",
    "    if not phone:\n",
    "        return False\n",
    "    cleaned = phone.replace(' ', '').replace('-', '')\n",
    "    if cleaned.startswith('+91'):\n",
    "        core = cleaned[3:]\n",
    "    elif cleaned.startswith('91'):\n",
    "        core = cleaned[2:]\n",
    "    else:\n",
    "        core = cleaned\n",
    "    if not re.fullmatch(r'\\d{10}', core):\n",
    "        return False\n",
    "    num = int(core)\n",
    "    return 6000000000 <= num <= 9999999999\n",
    "\n",
    "with open('DataEngineeringQ2.json') as f:\n",
    "    data = json.load(f)\n",
    "\n",
    "valid_phones = 0\n",
    "records = []\n",
    "\n",
    "for entry in data:\n",
    "    phone = entry.get('phoneNumber')\n",
    "    if is_valid_phone_number(phone):\n",
    "        valid_phones += 1\n",
    "    patient_details = entry.get('patientDetails', {})\n",
    "    if (birth_date_str := patient_details.get('birthDate')):\n",
    "        birth_date = datetime.fromisoformat(birth_date_str.replace('Z', '+00:00'))\n",
    "        age = (datetime.now(timezone.utc) - birth_date).days / 365.25\n",
    "        medicines = entry['consultationData'].get('medicines', [])\n",
    "        active_meds = sum(1 for med in medicines if med.get('isActive', False))\n",
    "        records.append({'age': age, 'active_meds': active_meds})\n",
    "\n",
    "print(f\"Valid phone numbers: {valid_phones}\")\n",
    "\n",
    "df = pd.DataFrame(records)\n",
    "if not df.empty:\n",
    "    corr, p_value = pearsonr(df['age'], df['active_meds'])\n",
    "    print(f\"\\nAge-Medication Correlation: {corr:.3f}\")\n",
    "    print(f\"P-value: {p_value:.3f}\")\n",
    "else:\n",
    "    print(\"\\nNo valid age/medication data for correlation analysis\")\n"
   ]
