    "import json\n",
    "from datetime import datetime, timezone\n",
    "import pandas as pd\n",
    "from scipy.stats import pearsonr\n",
    "\n",
    "\n",
    "with open('DataEngineeringQ2.json') as f:\n",
    "    data = json.load(f)  
    "\n",
    "records = []\n",
    "for entry in data:\n",
    "   
    "    patient_details = entry.get('patientDetails', {})\n",
    "    birth_date_str = patient_details.get('birthDate')\n",
    "    \n",
    "    if not birth_date_str:\n",
    "        continue  
    "        \n",
    "    # Calculate age\n",
    "    birth_date = datetime.fromisoformat(birth_date_str.replace('Z', '+00:00'))\n",
    "    age = (datetime.now(timezone.utc) - birth_date).days / 365.25\n",
    "    \n",
    "    # Count active medications\n",
    "    medicines = entry['consultationData'].get('medicines', [])\n",
    "    active_meds = sum(1 for med in medicines if med.get('isActive', False))\n",
    "    \n",
    "    records.append({'age': age, 'active_meds': active_meds})\n",
    "\n",
    "# Create DataFrame and calculate correlation\n",
    "df = pd.DataFrame(records)\n",
    "correlation, p_value = pearsonr(df['age'], df['active_meds'])\n",
    "\n",
    "print(f\"Pearson Correlation: {correlation:.3f}\")\n",
    "print(f\"P-value: {p_value:.3f}\")\n"
   ]
